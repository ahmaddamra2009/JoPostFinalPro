@model IEnumerable<Instructor>
<!-- Our Agent Section-->
<section id="aa-agents">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="aa-agents-area">
                    <div class="aa-title">
                        <h2>Our Insructor</h2>
                        <span></span>
                        <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Cum sit ea nobis quae vero voluptatibus.</p>
                    </div>
                    <!-- agents content -->
                    <div class="aa-agents-content">
                        <ul class="aa-agents-slider">
                            @foreach (var item in Model)
                            {
                            <li>
                                <div class="aa-single-agents">
                                    <div class="aa-agents-img">
                                        <img src="~/img/agents/agent-1.png" alt="agent member image">
                                    </div>
                                    <div class="aa-agetns-info">
                                        <h4><a href="#">@item.InsName</a></h4>
                                        <span>@item.Position</span>
                                        <div class="aa-agent-social">
                                            @if (item.FB != null)
                                            {
                                                <a href="@item.FB"><i class="fa fa-facebook"></i></a>
                                            }
                                            @if (item.TW != null)
                                            {
                                                <a href="@item.TW"><i class="fa fa-twitter"></i></a>
                                            }
                                            @if (item.Ins != null)
                                            {
                                                <a href="@item.Ins"><i class="fa fa-instagram"></i></a>
                                            }
                                            @if (item.linkedIn != null)
                                            {
                                        <a href="@item.linkedIn"><i class="fa fa-linkedin"></i></a>
                                            }

                                            

                                        </div>
                                    </div>
                                </div>
                            </li>
                            }
                            

                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- / Our Agent Section-->